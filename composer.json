{
    "name": "lynkbyte/docs-builder",
    "description": "A Laravel-powered static documentation site generator that compiles Markdown and OpenAPI YAML into themed, searchable HTML.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "documentation",
        "static-site",
        "markdown",
        "openapi"
    ],
    "authors": [
        {
            "name": "LynkByte",
            "email": "dev@lynkbyte.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/view": "^11.0|^12.0",
        "league/commonmark": "^2.7",
        "symfony/yaml": "^6.4|^7.0|^8.0",
        "tempest/highlight": "^2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.27",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "LynkByte\\DocsBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LynkByte\\DocsBuilder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LynkByte\\DocsBuilder\\DocsBuilderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
